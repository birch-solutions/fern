{
  "type": "library",
  "commonJs": true,
  "private": false,
  "rules": {
    "duplicate-dependencies": {
      "exclude": [
        "@fern-fern/ir-sdk"
      ]
    },
    "depcheck": {
      "ignores": [
        "@types/jest",
        "@types/node",
        "node-fetch",
        "qs",
        "@types/node-fetch",
        "@types/qs",
        "jest-environment-jsdom"
      ]
    },
    "package-json": {
      "scripts": {
        "test": "yarn compile && jest --passWithNoTests && jest --passWithNoTests --env jsdom"
      }
    }
  }
}