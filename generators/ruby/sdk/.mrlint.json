{
  "type": "cli",
  "cliName": "fern-ruby-sdk",
  "environment": {
    "environments": {
      "prod": {
        "cliName": "fern-ruby-sdk",
        "cliPackageName": "@fern-api/fern-ruby-sdk"
      }
    }
  },
  "rules": {
    "depcheck": {
      "ignores": ["webpack", "webpack-cli", "ts-loader", "@fern-fern/ir-sdk"],
      "ignore-patterns": ["docker/bundle.js"]
    },
    "duplicate-dependencies": {
      "exclude": ["@fern-fern/ir-sdk"]
    }
  },
  "private": false
}
