{
    "irFilepath": "./ir.json",
    "output": {
        "mode": {
            "repoUrl": "https://github.com/exhaustive/fern",
            "version": "0.0.1",
            "publishInfo": {
                "registryUrl": "",
                "packageName": "@fern/exhaustive",
                "tokenEnvironmentVariable": "",
                "type": "npm"
            },
            "type": "github"
        },
        "path": "../"
