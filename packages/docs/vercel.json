{
  "rewrites": [
    {
      "source": "/hello-world",
      "destination": "https://fern.mintlify.dev/docs"
    },
    {
      "source": "/docs",
      "destination": "https://fern.mintlify.dev/docs"
    },
    {
      "source": "/docs/:match*",
      "destination": "https://fern.mintlify.dev/docs/:match*"
    }
  ],
  "github": {
    "silent": true
  }
}
