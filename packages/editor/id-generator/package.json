    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/uuid": "^8.3.4",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "typescript": "4.6.4"
