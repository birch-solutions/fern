    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/uuid": "^9.0.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
