{
  "github": {
    "silent": true
  },
  "git": {
    "deploymentEnabled": false
  },
  "rewrites": [
    {
      "has": [
        {
          "type": "header",
          "key": "x-fern-host",
          "value": "(?<host>.*)"
        }
      ],
      "source": "/",
      "destination": "/:host"
    },
    {
      "has": [
        {
          "type": "header",
          "key": "x-fern-host",
          "value": "(?<host>.*)"
        }
      ],
      "source": "/:path*",
      "destination": "/:host/:path*"
    }
  ]
}
