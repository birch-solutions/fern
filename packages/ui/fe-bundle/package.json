{
  "name": "@fern-api/fe-bundle",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/ui/fe-bundle"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "yarn lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "start": "yarn compile && vite",
    "build": "yarn compile && vite build",
    "preview": "yarn compile && vite preview",
    "deploy:dev": "yarn build --mode staging && FERN_CLOUD_ENVIRONMENT=DEV cdk deploy --output deploy/cdk.out --require-approval never --progress events",
    "deploy:production": "yarn build --mode production && FERN_CLOUD_ENVIRONMENT=PROD cdk deploy --output deploy/cdk.out --require-approval never --progress events"
  },
  "dependencies": {
    "@fern-api/ui": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@fern-api/docs-deploy": "workspace:*",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "aws-cdk": "^2.80.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "jest": "^29.0.3",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "stylelint": "^14.14.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.6.4",
    "vite": "^4.3.8",
    "vite-plugin-checker": "^0.5.5"
  }
}
