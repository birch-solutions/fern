{
  "name": "@fern-api/next-app",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/ui/next-app"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "source": "src/index.ts",
  "module": "src/index.ts",
  "main": "src/index.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:style": "stylelint 'src/**/*.scss' --allow-empty-input --max-warnings 0",
    "lint:style:fix": "yarn lint:style --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": "depcheck",
    "dev": "yarn compile && next dev",
    "build": "yarn compile && next build",
    "start": "yarn compile && next start",
    "lint": "yarn compile && next lint"
  },
  "dependencies": {
    "@fern-api/ui": "workspace:*",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.13",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "jest": "^29.4.0",
    "organize-imports-cli": "^0.10.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "stylelint": "^14.14.0",
    "tailwindcss": "^3.2.7",
    "typescript": "4.6.4"
  }
}
