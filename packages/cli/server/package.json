{
  "name": "@fern-api/server",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/cli/server"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "bin": "./dist/bundle.cjs",
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore",
    "lint:eslint:fix": "eslint --max-warnings 0 . --ignore-path=../../../.eslintignore --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck",
    "dist:cli": "yarn compile && node build.cjs",
    "publish:cli": "yarn dist:cli && cd dist && yarn npm publish",
    "start": "yarn dist:cli && node dist/bundle.cjs"
  },
  "dependencies": {
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@fern-fern/ir-model": "0.0.914",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@yarnpkg/esbuild-plugin-pnp": "^3.0.0-rc.14",
    "depcheck": "^1.4.3",
    "esbuild": "^0.15.7",
    "eslint": "^8.26.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  }
}
