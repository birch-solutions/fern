{
  "name": "@fern-api/cli",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:fern-api/fern.git",
    "directory": "packages/cli"
  },
  "files": [
    "lib"
  ],
  "main": "./lib/cjs/index.js",
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "sideEffects": false,
  "bin": {
    "fern": "./cli"
  },
  "scripts": {
    "clean": "tsc --build --clean tsconfig.json && tsc --build --clean tsconfig.esm.json",
    "compile": "yarn run compile:cjs",
    "compile:all": "run-s compile:cjs compile:esm",
    "compile:cjs": "tsc --build tsconfig.json && echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json",
    "compile:esm": "tsc --build tsconfig.esm.json && echo '{ \"type\": \"module\" }' > lib/esm/package.json",
    "test": "yarn run compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore",
    "lint:eslint:fix": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck",
    "dist": "./webpack/bundle.sh $(yarn info @fern-api/cli --all --json | jq -r .children.Version)"
  },
  "dependencies": {
    "@fern-api/add-generator": "workspace:*",
    "@fern-api/commons": "workspace:*",
    "@fern-api/compiler": "workspace:*",
    "@fern-api/compiler-commons": "workspace:*",
    "@fern-api/init": "workspace:*",
    "@fern-api/local-workspace-runner": "workspace:*",
    "@fern-api/login": "workspace:*",
    "@fern-api/openapi-converter": "workspace:*",
    "@fern-api/remote-workspace-runner": "workspace:*",
    "@fern-api/syntax-analysis": "workspace:*",
    "chalk": "^4.1.2",
    "glob": "^7.2.0",
    "glob-promise": "^4.2.2",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.1.0",
    "validate-npm-package-name": "^4.0.0",
    "yargs": "^17.4.1",
    "zod": "^3.14.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.2",
    "@types/node": "^17.0.33",
    "@types/validate-npm-package-name": "^4.0.0",
    "@types/yargs": "^17.0.10",
    "depcheck": "^1.4.3",
    "eslint": "^8.12.0",
    "jest": "^28.1.1",
    "node-loader": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.6.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
